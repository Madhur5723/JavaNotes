import java.util.ArrayList;
import java.util.List;

public class CollectionTutorials {
	
	public static void main(String[] args) {
		List<Integer> list=new ArrayList<>();//<>-Generic,Integer-WrapperClass
		list.add(10);
		list.add(20);
		list.add(30);
//We can add 25 after 30 we have to give the index then the value we want to add	
		list.add(2, 25);
		list.add("Madhur");
		System.out.println(list);
	}

}
