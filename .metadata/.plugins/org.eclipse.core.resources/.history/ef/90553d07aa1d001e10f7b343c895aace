//<>-Generic,Integer-WrapperClass
import java.util.ArrayList;

public class CollectionTutorials {
	public static void main(String[] args) {
		//1-List
		//ArrayList
		ArrayList<Integer> arrList=new ArrayList<Integer>();
		arrList.add(10);
		arrList.add(20);
		arrList.add(30);
        //We can add 25 after 30 we have to give the index then the value we want to add	
		arrList.add(2, 25);
		System.out.println(arrList);
		////////////////////////////////////////////////////////////////////////////////////
		
		//LinkedList
		LinkedList<Integer> =new LinkedList<Integer>();
	}

}
