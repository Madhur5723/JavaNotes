//<>-Generic,Integer-WrapperClass
import java.util.ArrayList;

public class CollectionTutorials {
	public static void main(String[] args) {
		//ArrayList
		ArrayList<Integer> list=new ArrayList<Integer>();
		list.add(10);
		list.add(20);
		list.add(30);
        //We can add 25 after 30 we have to give the index then the value we want to add	
		list.add(2, 25);
		System.out.println(list);
		////////////////////////////////////////////////////////////////////////////////////
		
		//LinkedList
		ArrayList<Integer> list=new ArrayList<Integer>();//<>-Generic,Integer-WrapperClass
		list.add(10);
		list.add(20);
		list.add(30);
		list.add(2, 25);
		System.out.println(list);
		
	}

}
