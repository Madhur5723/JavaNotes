import java.util.HashMap;
import java.util.TreeMap;

public class Map {

	public static void main(String[] args) {
		//4-Map-To store group of values,key,value ,keyValue pair
		//<Key,Value> key =uniqueValue ,Value=duplicateValue
		
		//HashMap=will give output in order
		HashMap<String,Integer> hmap=new HashMap<>();
		hmap.put("Second",20);
		hmap.put("First",10);
		System.out.println("HashMap="+hmap+"\n");
		////////////////////////////////////////////////////
		
		//TreeMap=Will give 
		TreeMap<String,Integer> tmap=new TreeMap<>();
		tmap.put("Second",20);
		tmap.put("First",10);
		System.out.println("TreeMap="+tmap+"\n");
		


	}

}
