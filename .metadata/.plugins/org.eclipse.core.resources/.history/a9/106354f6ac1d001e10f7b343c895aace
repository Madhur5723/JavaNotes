//<>-Generic,Integer-WrapperClass
import java.util.ArrayList;
import java.util.LinkedList;
import java.util.Stack;
import java.util.Vector;

public class CollectionTutorials {
	public static void main(String[] args) {
		//1-List
		//ArrayList
		ArrayList<Integer> arrList=new ArrayList<Integer>();
		arrList.add(10);
		arrList.add(20);
		arrList.add(30);
        //We can add 25 after 30 we have to give the index then the value we want to add	
		arrList.add(2, 25);
		System.out.println("ArrayList="+arrList+"\n");
		////////////////////////////////////////////////////////////////////////////////////
		
		//LinkedList
		LinkedList<Integer> linkList =new LinkedList<Integer>();
		linkList.add(0,10);
		linkList.add(20);
		linkList.add(30);
		linkList.remove(1);//give the index to remove element
		System.out.println("LinkedList="+linkList+"\n");
		////////////////////////////////////////////////////////////////////////////////////
		
		//Vector
		Vector<String> vec =new Vector<String>();
		vec.add("Madhur");
		vec.add("Hari");
		vec.add("Omkar");
		System.out.println("Vector="+vec+"\n");
		////////////////////////////////////////////////////////////////////////////////
		
		//Stack-LIFO
		Stack<Integer> stack=new Stack<Integer>();
		stack.push(10);
		stack.push(10);//It can hold duplicate value also
		stack.push(20);
		System.out.println("Stack="+stack);
		System.out.println("TopValue+"+stack.peek());


	}

}	
